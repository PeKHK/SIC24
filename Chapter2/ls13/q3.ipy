n=int(input('Enter n:'))
list1=[]
#create list
for i in range(n):
    line=[]
    for j in range(n):
        if (i+j)%2 == 0:#row+column divide 2 and get Nmuerator 0
            line.append(1)
        else:
            line.append(0)
    list1.append(line)
   
#display
for i in range(len(list1)):
    for j in range(len(list1[i])):
        print(list1[i][j],end=' ')
    print()