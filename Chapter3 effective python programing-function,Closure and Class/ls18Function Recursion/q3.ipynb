   "source": [
    "#find e of log\n",
    "def factorial(k):\n",
    "    if k == 0:\n",
    "        return 1\n",
    "    elif k==1:\n",
    "        return 1\n",
    "    else:\n",
    "        return k*factorial(k-1)\n",
    "def euler(n):\n",
    "    if n==0:\n",
    "        return 1\n",
    "    else:\n",
    "        return 1/factorial(n) + euler(n-1)\n",
    "\n",
    "print(f'euler(20)={euler(20):.5f}')"
   ]
  }
