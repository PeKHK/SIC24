import random
correctNum=random.randint(1,100)
total_try=0
print('Guess a number between 1 to 100')
while True:
    num=int(input('Enter a number: '))
    total_try+=1
    if num<correctNum:
        print('Highter')
    elif num>correctNum:
        print('Lower')
    else:
        print('Congratulation. Total try =',total_try)
        break